{
  "extends": "airbnb-base",
  "plugins": [
      "import"
  ],
  "globals": {
    "ENV": true,
    "d3": false,
    "document": true
  },
  "env": {
    "browser": true
  },
  "rules": {
    "camelcase": 0,
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "no-underscore-dangle": 0,
    "no-mixed-operators": 0,
    "function-paren-newline": 0,
    "func-names": 0,
    "prefer-destructuring": 0,
    "object-curly-newline": 0,
    "object-shorthand": 0
  }
}
